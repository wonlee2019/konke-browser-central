{
  "name": "Startpage",
  "description": "Search Startpage",
  "manifest_version": 2,
  "version": "1.0",
  "applications": {
    "gecko": {
      "id": "startpage@search.dothq.co"
    }
  },
  "hidden": true,
  "icons": {
    "16": "favicon.png"
  },
  "web_accessible_resources": [
    "favicon.png"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": [
        "@startpage",
        "@sp"
      ],
      "name": "Startpage",
      "search_url": "https://startpage.com/do/dsearch",
      "search_url_get_params": "q={searchTerms}",
      "params": [],
      "suggest_url": "https://ac.duckduckgo.com/ac/",
      "suggest_url_get_params": "q={searchTerms}&type=list"
    }
  }
}