{
  "name": "Bing",
  "description": "Microsoft Bing",
  "manifest_version": 2,
  "version": "1.3",
  "applications": {
    "gecko": {
      "id": "bing@search.dothq.co"
    }
  },
  "hidden": true,
  "icons": {
    "16": "favicon.ico"
  },
  "web_accessible_resources": [
    "favicon.ico"
  ],
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "@bing",
      "name": "Bing",
      "search_url": "https://www.bing.com/search",
      "search_form": "https://www.bing.com/search?q={searchTerms}",
      "search_url_get_params": "q={searchTerms}",
      "params": [],
      "suggest_url": "https://www.bing.com/osjson.aspx",
      "suggest_url_get_params": "query={searchTerms}&form=OSDJAS&language={moz:locale}"
    }
  }
}